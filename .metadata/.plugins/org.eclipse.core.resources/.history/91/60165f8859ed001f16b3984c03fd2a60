class Prac implements Runnable{
	
	public void run() {
			try {
				
				for (int i = 0; i < 10 ; i++) {
					System.out.println("Inside run method !!!");
						Thread.sleep(2000);
				}
				
			} catch (Exception e) {
		}
	}
	
}
public class Demo {
	public static void main(String[] args) {
		Prac prac = new Prac();
		Thread thread = new Thread(prac , "By Custome thread created");
		thread.start();
		thread.interrupt();
		for(int i = 0; i < 10 ; i++) {
			System.out.println("Inside main method !!!");
		}
	}
}
